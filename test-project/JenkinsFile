pipeline{
 agent any
 stages {
	stage ('Unit Test'){
 		steps {
 			echo 'We do not have test cases'
 		}
 	}
 	stage ('Build'){
 		steps {
 			withMaven(maven:'maven'){
 				sh 'mvn -f test-project/pom.xml clean install'
 			}
 		}
 	}
 	stage ('Deploy'){
 		steps {
 			withMaven(maven:'maven'){
 				sh 'mvn -f test-project/pom.xml package deploy  -Dusername=susimule -Dpassword=Ankit123 -Denvironment=Sandbox -DmuleDeploy'
 			}
 		}
 	}
 }

}